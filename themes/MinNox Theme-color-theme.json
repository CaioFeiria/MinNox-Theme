      "settings": { "fontStyle": "", "foreground": "#d6d3d1" }
    },
    { "scope": ["constant"], "settings": { "foreground": "#cfb574" } },
    {
      "scope": ["string"],
      "settings": { "foreground": "#bef264", "fontStyle": "" }
